        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/i6"
            android:layout_alignTop="@+id/i5"
            android:layout_alignLeft="@+id/i3"
            android:layout_alignStart="@+id/i3" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/i4"
            android:layout_alignTop="@+id/i5"
            android:layout_alignRight="@+id/i1"
            android:layout_alignEnd="@+id/i1" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/champion_string"
            android:id="@+id/t6"
            android:layout_above="@+id/i4"
            android:layout_alignRight="@+id/i4"
            android:layout_alignEnd="@+id/i4" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
