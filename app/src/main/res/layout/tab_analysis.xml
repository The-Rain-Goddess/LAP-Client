            android:layout_toStartOf="@+id/t1" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/champion_string"
            android:id="@+id/t4"
            android:layout_above="@+id/i1"
            android:layout_centerHorizontal="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/champion_string"
            android:id="@+id/t5"
            android:layout_above="@+id/i3"
            android:layout_alignLeft="@+id/i3"
            android:layout_alignStart="@+id/i3" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
