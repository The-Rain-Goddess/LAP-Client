            android:layout_alignRight="@+id/i7"
            android:layout_alignEnd="@+id/i7" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/champion_string"
            android:id="@+id/t10"
            android:layout_above="@+id/i9"
            android:layout_alignLeft="@+id/t7"
            android:layout_alignStart="@+id/t7" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/champion_string"
            android:id="@+id/t11"
            android:layout_above="@+id/i9"
            android:layout_alignLeft="@+id/i9"
            android:layout_alignStart="@+id/i9" />


