<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent" android:layout_height="wrap_content"
        android:minHeight="700dp"
        android:background="@color/win_color"
        android:id="@+id/champion_view"
        >

        <include
            android:id="@+id/tool_bar"
            layout="@layout/tool_bar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            >
        </include>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/champion_view_basic_stats">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tet1"
                />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/ahri"
                android:layout_marginStart="29dp"
                android:layout_marginTop="17dp"
                android:id="@+id/champion_view_portrait"
                android:layout_below="@+id/champion_view_champname"
                android:layout_toEndOf="@+id/tet1" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Champion Name"
                android:ems="10"
                android:id="@+id/champion_view_champname"
                android:elevation="5dp"
                android:contentDescription="@string/app_name"
                android:editable="false"
                style="@style/Base.TextAppearance.AppCompat.Large"
                android:textAlignment="center"
                android:textAllCaps="true"
                android:paddingTop="20dp"
                android:textSize="30sp"
                android:layout_marginTop="55dp"
                android:layout_alignParentStart="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="###"
                android:textSize="25sp"
                android:layout_alignTop="@+id/champion_view_portrait"
                android:layout_toEndOf="@+id/champion_view_portrait"
                android:id="@+id/num"
                android:paddingStart="35dp"
                android:paddingEnd="5dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/champion_view_portrait"
                android:layout_toEndOf="@+id/num"
                android:textSize="25sp"
                android:id="@+id/champion_view_staticplayed"
                android:text="@string/num_played"/>

            <TextView
                android:text="##"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/num"
                android:layout_toEndOf="@+id/champion_view_portrait"
                android:id="@+id/champion_view_percentWon"
                android:paddingStart="45dp"
                android:paddingEnd="0dp"
                android:textSize="25sp"
                android:paddingTop="10dp"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/num"
                android:layout_toEndOf="@+id/champion_view_percentWon"
                android:paddingTop="10dp"
                android:textSize="25sp"
                android:text="@string/percent_won"
                />

            <TextView
                android:text="###"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/champion_view_percentWon"
                android:layout_toEndOf="@+id/champion_view_portrait"
                android:id="@+id/champion_view_numberWon"
                android:paddingStart="35dp"
                android:paddingEnd="0dp"
                android:textSize="25sp"
                android:paddingTop="10dp"
                android:textColor="@color/good_green"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/champion_view_percentWon"
                android:layout_toEndOf="@+id/champion_view_numberWon"
                android:paddingTop="10dp"
                android:textSize="25sp"
                android:text="@string/num_won"
                android:id="@+id/champion_view_staticwon"
                android:textColor="@color/good_green"
                />

            <TextView
                android:text="###"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/champion_view_percentWon"
                android:layout_toEndOf="@+id/champion_view_staticwon"
                android:id="@+id/champion_view_numberLost"
                android:paddingStart="10dp"
                android:paddingEnd="0dp"
                android:textSize="25sp"
                android:paddingTop="10dp"
                android:textColor="@color/regular_red"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/champion_view_percentWon"
                android:layout_toEndOf="@+id/champion_view_numberLost"
                android:paddingTop="10dp"
                android:textSize="25sp"
                android:text="@string/num_lost"
                android:id="@+id/champion_view_staticLost"
                android:textColor="@color/regular_red"
                />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/champion_view_overall_stats_per_game"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/champion_view_basic_stats">

            <View
                android:id="@+id/champion_view_overall_stats_per_game_divider"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_marginStart="35dp"
                android:layout_marginEnd="35dp"
                android:background="@android:color/black"/>

            <TextView
                android:id="@+id/champion_view_overall_stats_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/champion_view_overall_stats_per_game_divider"
                android:layout_centerHorizontal="true"
                android:textColor="@color/black"
                android:textSize="20sp"
                android:text="@string/champion_view_overall_stats"/>

            <View
                android:id="@+id/champion_view_overall_stats_vertical_divider"
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:layout_below="@id/champion_view_overall_stats_title"
                android:layout_centerHorizontal="true"
                android:layout_margin="5dp">
            </View>

            <TextView
                android:id="@+id/champion_view_average_kda_numbers"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/champion_view_overall_stats_title"
                android:textSize="@dimen/champion_view_average_stats_text_size"
                android:layout_toStartOf="@id/champion_view_overall_stats_vertical_divider"
                android:text="@string/number"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/champion_view_average_kda_desc"
                android:layout_below="@id/champion_view_overall_stats_title"
                android:layout_toEndOf="@id/champion_view_overall_stats_vertical_divider"
                android:text="@string/champion_view_overall_stats_desc"
                android:textSize="@dimen/champion_view_average_stats_text_size"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/champion_view_average_kills_numbers"
                android:textSize="@dimen/champion_view_average_stats_text_size"
                android:layout_below="@id/champion_view_average_kda_numbers"
                android:layout_toStartOf="@id/champion_view_overall_stats_vertical_divider"
                android:text="@string/number"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/champion_view_average_kills_desc"
                android:layout_below="@id/champion_view_average_kda_desc"
                android:layout_toEndOf="@id/champion_view_overall_stats_vertical_divider"
                android:text="@string/champion_view_average_kills_desc"
                android:textSize="@dimen/champion_view_average_stats_text_size"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/champion_view_average_deaths_numbers"
                android:textSize="@dimen/champion_view_average_stats_text_size"
                android:layout_below="@id/champion_view_average_kills_numbers"
                android:layout_toStartOf="@id/champion_view_overall_stats_vertical_divider"
                android:text="@string/number"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/champion_view_average_deaths_desc"
                android:layout_below="@id/champion_view_average_kills_desc"
                android:layout_toEndOf="@id/champion_view_overall_stats_vertical_divider"
                android:text="@string/champion_view_average_deaths_desc"
                android:textSize="@dimen/champion_view_average_stats_text_size"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/champion_view_average_assists_numbers"
                android:textSize="@dimen/champion_view_average_stats_text_size"
                android:layout_below="@id/champion_view_average_deaths_numbers"
                android:layout_toStartOf="@id/champion_view_overall_stats_vertical_divider"
                android:text="@string/number"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/champion_view_average_assists_desc"
                android:layout_below="@id/champion_view_average_deaths_desc"
                android:layout_toEndOf="@id/champion_view_overall_stats_vertical_divider"
                android:text="@string/champion_view_average_assists_desc"
                android:textSize="@dimen/champion_view_average_stats_text_size"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/champion_view_average_cs_numbers"
                android:layout_marginTop="5dp"
                android:textSize="@dimen/champion_view_average_stats_text_size"
                android:layout_below="@id/champion_view_average_assists_numbers"
                android:layout_toStartOf="@id/champion_view_overall_stats_vertical_divider"
                android:text="@string/number"/>

            <TextView
                android:layout_marginTop="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/champion_view_average_cs_desc"
                android:layout_below="@id/champion_view_average_assists_desc"
                android:layout_toEndOf="@id/champion_view_overall_stats_vertical_divider"
                android:text="@string/champion_view_average_cs_desc"
                android:textSize="@dimen/champion_view_average_stats_text_size"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/champion_view_average_gold_numbers"
                android:textSize="@dimen/champion_view_average_stats_text_size"
                android:layout_below="@id/champion_view_average_cs_numbers"
                android:layout_toStartOf="@id/champion_view_overall_stats_vertical_divider"
                android:text="@string/number"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/champion_view_average_gold_desc"
                android:layout_below="@id/champion_view_average_cs_desc"
                android:layout_toEndOf="@id/champion_view_overall_stats_vertical_divider"
                android:text="@string/champion_view_average_gold_desc"
                android:textSize="@dimen/champion_view_average_stats_text_size"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/champion_view_average_damage_numbers"
                android:layout_marginTop="5dp"
                android:textSize="@dimen/champion_view_average_stats_text_size"
                android:layout_below="@id/champion_view_average_gold_numbers"
                android:layout_toStartOf="@id/champion_view_overall_stats_vertical_divider"
                android:text="@string/number"/>

            <TextView
                android:layout_marginTop="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/champion_view_average_damage_desc"
                android:layout_below="@id/champion_view_average_gold_desc"
                android:layout_toEndOf="@id/champion_view_overall_stats_vertical_divider"
                android:text="@string/champion_view_average_damage_desc"
                android:textSize="@dimen/champion_view_average_stats_text_size"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/champion_view_average_pdamage_numbers"
                android:textSize="@dimen/champion_view_average_stats_text_size"
                android:layout_below="@id/champion_view_average_damage_numbers"
                android:layout_toStartOf="@id/champion_view_overall_stats_vertical_divider"
                android:text="@string/number"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/champion_view_average_pdamage_desc"
                android:layout_below="@id/champion_view_average_damage_desc"
                android:layout_toEndOf="@id/champion_view_overall_stats_vertical_divider"
                android:text="@string/champion_view_average_pdamage_desc"
                android:textSize="@dimen/champion_view_average_stats_text_size"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/champion_view_average_mdamage_numbers"
                android:textSize="@dimen/champion_view_average_stats_text_size"
                android:layout_below="@id/champion_view_average_pdamage_numbers"
                android:layout_toStartOf="@id/champion_view_overall_stats_vertical_divider"
                android:text="@string/number"/>

            <TextView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/champion_view_average_mdamage_desc"
                android:layout_below="@id/champion_view_average_pdamage_desc"
                android:layout_toEndOf="@id/champion_view_overall_stats_vertical_divider"
                android:text="@string/champion_view_average_mdamage_desc"
                android:textSize="@dimen/champion_view_average_stats_text_size"/>

            <TextView
                android:layout_marginTop="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/champion_view_average_dtaken_numbers"
                android:textSize="@dimen/champion_view_average_stats_text_size"
                android:layout_below="@id/champion_view_average_mdamage_numbers"
                android:layout_toStartOf="@id/champion_view_overall_stats_vertical_divider"
                android:text="@string/number"/>

            <TextView
                android:layout_marginTop="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/champion_view_average_dtaken_desc"
                android:layout_below="@id/champion_view_average_mdamage_desc"
                android:layout_toEndOf="@id/champion_view_overall_stats_vertical_divider"
                android:text="@string/champion_view_average_dtaken_desc"
                android:textSize="@dimen/champion_view_average_stats_text_size"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/champion_view_average_heals_numbers"
                android:textSize="@dimen/champion_view_average_stats_text_size"
                android:layout_below="@id/champion_view_average_dtaken_numbers"
                android:layout_toStartOf="@id/champion_view_overall_stats_vertical_divider"
                android:text="@string/number"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/champion_view_average_heals_desc"
                android:layout_below="@id/champion_view_average_dtaken_desc"
                android:layout_toEndOf="@id/champion_view_overall_stats_vertical_divider"
                android:text="@string/champion_view_average_heals_desc"
                android:textSize="@dimen/champion_view_average_stats_text_size"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/champion_view_average_wards_numbers"
                android:textSize="@dimen/champion_view_average_stats_text_size"
                android:layout_below="@id/champion_view_average_heals_numbers"
                android:layout_toStartOf="@id/champion_view_overall_stats_vertical_divider"
                android:text="@string/number"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/champion_view_average_wards_desc"
                android:layout_below="@id/champion_view_average_heals_desc"
                android:layout_toEndOf="@id/champion_view_overall_stats_vertical_divider"
                android:text="@string/champion_view_average_wards_desc"
                android:textSize="@dimen/champion_view_average_stats_text_size"/>

            <View
                android:id="@+id/champion_view_average_bottom_divider"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@id/champion_view_average_wards_desc"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_marginEnd="60dp"
                android:layout_marginStart="60dp"
                android:background="@android:color/darker_gray">
            </View>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/champion_view_average_desc_divider"
                android:layout_below="@id/champion_view_average_bottom_divider"
                android:layout_centerHorizontal="true">

                <TextView
                    android:id="@+id/champion_view_overall_middle_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="5dp"
                    android:textSize="15sp"
                    android:textColor="@color/black"
                    android:text="@string/champion_view_overall_middle_title"/>

                <TextView
                    android:layout_marginTop="11dp"
                    android:id="@+id/champion_view_overall_middle_kda"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/champion_view_overall_middle_title"
                    android:text="@string/champion_view_overall_middle_kda"
                    android:layout_centerHorizontal="true"/>

                <TextView
                    android:id="@+id/champion_view_overall_middle_kills"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/champion_view_overall_middle_kda"
                    android:layout_centerHorizontal="true"
                    android:text="@string/champion_view_overall_middle_kills"/>

                <TextView
                    android:id="@+id/champion_view_overall_middle_deaths"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/champion_view_overall_middle_kills"
                    android:layout_centerHorizontal="true"
                    android:text="@string/champion_view_overall_middle_deaths"/>

                <TextView
                    android:id="@+id/champion_view_overall_middle_assists"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/champion_view_overall_middle_deaths"
                    android:layout_centerHorizontal="true"
                    android:text="@string/champion_view_overall_middle_assists"/>

                <TextView
                    android:layout_marginTop="5dp"
                    android:id="@+id/champion_view_overall_middle_cs"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/champion_view_overall_middle_assists"
                    android:layout_centerHorizontal="true"
                    android:text="@string/champion_view_overall_middle_cs"/>

                <TextView
                    android:id="@+id/champion_view_overall_middle_gold"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/champion_view_overall_middle_cs"
                    android:layout_centerHorizontal="true"
                    android:text="@string/champion_view_overall_middle_gold"/>

                <TextView
                    android:layout_marginTop="5dp"
                    android:id="@+id/champion_view_overall_middle_damage"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/champion_view_overall_middle_gold"
                    android:layout_centerHorizontal="true"
                    android:text="@string/champion_view_overall_middle_damage"/>

                <TextView
                    android:id="@+id/champion_view_overall_middle_pdamage"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/champion_view_overall_middle_damage"
                    android:layout_centerHorizontal="true"
                    android:text="@string/champion_view_overall_middle_pdamage"/>

                <TextView
                    android:id="@+id/champion_view_overall_middle_mdamage"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/champion_view_overall_middle_pdamage"
                    android:layout_centerHorizontal="true"
                    android:text="@string/champion_view_overall_middle_mdamage"/>

                <TextView
                    android:layout_marginTop="5dp"
                    android:id="@+id/champion_view_overall_middle_dtaken"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/champion_view_overall_middle_mdamage"
                    android:layout_centerHorizontal="true"
                    android:text="@string/champion_view_overall_middle_dtaken"/>

                <TextView
                    android:id="@+id/champion_view_overall_middle_heals"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/champion_view_overall_middle_dtaken"
                    android:layout_centerHorizontal="true"
                    android:text="@string/champion_view_overall_middle_heals"/>

                <TextView
                    android:id="@+id/champion_view_overall_middle_wards"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/champion_view_overall_middle_heals"
                    android:layout_centerHorizontal="true"
                    android:text="@string/champion_view_overall_middle_wards"
                    android:layout_marginBottom="5dp"/>



            </RelativeLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/champion_view_average_bottom_divider"
                android:id="@+id/champion_view_average_victory_stats"
                android:layout_toStartOf="@id/champion_view_average_desc_divider">

                <TextView
                    android:id="@+id/champion_view_average_victory_title"
                    android:layout_marginEnd="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/champion_view_victory"
                    android:textSize="20sp"
                    android:textColor="@color/good_green"
                    android:layout_centerHorizontal="true"
                    />

                <TextView
                    android:id="@+id/champion_view_overall_victory_kda"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="9dp"
                    android:layout_below="@id/champion_view_average_victory_title"
                    android:text="@string/number"/>

                <TextView
                    android:id="@+id/champion_view_overall_victory_kills"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_below="@id/champion_view_overall_victory_kda"
                    android:text="@string/number"/>

                <TextView
                    android:id="@+id/champion_view_overall_victory_deaths"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_below="@id/champion_view_overall_victory_kills"
